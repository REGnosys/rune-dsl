delimiters "<", ">"

scopeClass(scopeName, overrides) ::= <<
public class <scopeName> extends AbstractRuneScope {
  @Override
  protected void configure() {
    <overrides:{o | addOverride(<typeRef(o.from)>, <typeRef(o.to)>);}; separator="\n">
  }
}
>>

typeRef(clazz) ::= <<
<clazz.simpleName>.class
>>
